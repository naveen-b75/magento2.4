"CREATE TABLE `phpcuong_banners_slider` (\n  `id` int unsigned NOT NULL AUTO_INCREMENT COMMENT 'Banner ID',\n  `name` varchar(255) DEFAULT NULL COMMENT 'Banner Name',\n  `url` varchar(255) DEFAULT NULL COMMENT 'URL Link',\n  `image` varchar(255) NOT NULL COMMENT 'Banner Image',\n  `status` smallint NOT NULL DEFAULT '1' COMMENT 'Banner Status: Enabled is 1 and Disabled is 2',\n  `store_id` smallint unsigned NOT NULL COMMENT 'Store Id',\n  `order` smallint DEFAULT NULL COMMENT 'Sort Order',\n  `group_id` smallint unsigned NOT NULL COMMENT 'Group Id',\n  `creation_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP COMMENT 'Banner Creation Time',\n  `update_time` timestamp NOT NULL DEFAULT CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP COMMENT 'Banner Modification Time',\n  PRIMARY KEY (`id`),\n  KEY `PHPCUONG_BANNERS_SLIDER_STORE_ID_STORE_STORE_ID` (`store_id`),\n  KEY `FK_55B7ECC31719E7F9089DACFE796C26BA` (`group_id`),\n  FULLTEXT KEY `PHPCUONG_BANNERS_SLIDER_NAME` (`name`),\n  CONSTRAINT `FK_55B7ECC31719E7F9089DACFE796C26BA` FOREIGN KEY (`group_id`) REFERENCES `phpcuong_banners_slider_group` (`id`),\n  CONSTRAINT `PHPCUONG_BANNERS_SLIDER_STORE_ID_STORE_STORE_ID` FOREIGN KEY (`store_id`) REFERENCES `store` (`store_id`)\n) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8mb3 COMMENT='Banners Slider Table'"